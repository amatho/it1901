<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CourseController">
  <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0">
         <center>
          <ToolBar id="1" fx:id="toolbar" prefHeight="55.0" prefWidth="329.0" BorderPane.alignment="CENTER">
            <items>
                  <TextField id="1" fx:id="playerName" editable="false" />
              <Button id="1" fx:id="plus" mnemonicParsing="false" onAction="#handleClick" text="+" />
              <TextField fx:id="textField" editable="false" prefHeight="25.0" prefWidth="49.0" />
              <Button id="1" fx:id="minus" mnemonicParsing="false" onAction="#handleClick" text="-" />
            </items>
          </ToolBar>
         </center>
      </BorderPane>
  </children>
</AnchorPane>
