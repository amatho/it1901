<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="golfapp.gui.UserController">
  <children>
    <Label fx:id="username" layoutX="264.0" layoutY="28.0" text="User">
      <font>
        <Font name="System Bold" size="36.0"/>
      </font>
    </Label>
    <Label layoutX="167.0" layoutY="96.0" text="Scorecard History:">
      <font>
        <Font name="System Bold" size="19.0"/>
      </font>
    </Label>
    <Button fx:id="viewSelectedScorecard" layoutX="438.0" layoutY="359.0" mnemonicParsing="false"
      onAction="#handleViewSelectedScorecardButton" prefHeight="27.0" prefWidth="153.0"
      text="View selected soorecard"/>
    <Button fx:id="logOut" layoutX="514.0" layoutY="14.0" mnemonicParsing="false"
      onAction="#handleLogOutButton" prefHeight="27.0" prefWidth="72.0" text="Log out"/>
    <TableView fx:id="scorecardTableView" layoutX="167.0" layoutY="126.0" prefHeight="271.0"
      prefWidth="266.0">
      <columns>
        <TableColumn fx:id="courseColumn" prefWidth="130.0" text="Course"/>
        <TableColumn fx:id="timeColumn" prefWidth="135.0" text="Time"/>
      </columns>
    </TableView>
  </children>
</AnchorPane>
