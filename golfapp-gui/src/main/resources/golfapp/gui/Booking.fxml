<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="golfapp.gui.BookingController">
   <children>
      <Label layoutX="300.0" layoutY="14.0" prefHeight="36.0" prefWidth="200.0" text="Booking">
         <font>
            <Font name="System Bold" size="40.0" />
         </font></Label>
      <Label layoutX="50.0" layoutY="120.0" text="Course:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="courseComboBox" layoutX="50.0" layoutY="150.0" prefHeight="25.0" prefWidth="198.0" />
      <Label layoutX="300.0" layoutY="120.0" prefHeight="18.0" prefWidth="131.0" text="Date:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="dateComboBox" layoutX="300.0" layoutY="150.0" prefHeight="25.0" prefWidth="176.0" />
      <Label fx:id="outputLabel" layoutX="550.0" layoutY="120.0" prefHeight="37.0" prefWidth="220.0" text="Choose a time:">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Label fx:id="yourBooking" layoutX="50.0" layoutY="270.0" prefHeight="37.0" prefWidth="198.0" text="Booking:">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <Button fx:id="confirmBooking" layoutX="640.0" layoutY="540.0" mnemonicParsing="false" onAction="#confirmBooking" prefHeight="37.0" prefWidth="131.0" text="Confirm booking">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
      </Button>
      <Label fx:id="yourCourseLabel" layoutX="50.0" layoutY="327.0" prefHeight="25.0" prefWidth="119.0" text="Course:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="yourDateLabel" layoutX="50.0" layoutY="362.0" prefHeight="25.0" prefWidth="119.0" text="Date:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="yourTimeLabel" layoutX="50.0" layoutY="397.0" prefHeight="25.0" prefWidth="119.0" text="Time:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="yourCourseText" layoutX="110.0" layoutY="327.0" prefHeight="25.0" prefWidth="141.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="yourDateText" layoutX="110.0" layoutY="362.0" prefHeight="25.0" prefWidth="141.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="yourTimeText" layoutX="110.0" layoutY="397.0" prefHeight="25.0" prefWidth="141.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="yourMailText" layoutX="110.0" layoutY="433.0" prefHeight="25.0" prefWidth="170.0">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="yourMailLabel" layoutX="50.0" layoutY="433.0" prefHeight="25.0" prefWidth="119.0" text="Mail:">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="confirmedBookingLabel" layoutX="300.0" layoutY="536.0" prefHeight="45.0" prefWidth="314.0">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <ComboBox fx:id="availableTimesComboBox" layoutX="550.0" layoutY="150.0" prefWidth="150.0" />
   </children>
</AnchorPane>
